        "paths": {
            "@co.mmons/js-utils": ["src"],
            "@co.mmons/js-utils/*": ["src/*"]
        },
        "sourceMap": true,
        "importHelpers": true,
        "plugins": [
            {"transform": "./dist/json/TsTransformer/index.js"}
        ]
    },
    "exclude": [
        "dist",
        "node_modules"
    ]
}
